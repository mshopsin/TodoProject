<h1>Projects</h1>

<ul>
  <% @projects.each do |project| %>
    <li><%= link_to "#{project.title}", project  %>
      <%= link_to " Remove ", project_path(project.id),
          method: :delete %>
        <p class="tab">
        <%= project.description %>
      </p>
    </li>
  <% end %>
</ul>

<%= link_to "New project", new_project_path %>